{
  "name": "@stephenchenorg/astro-graphql",
  "type": "module",
  "version": "9.0.0",
  "description": "Stephenchenorg Astro 前端 GraphQL 整合串接套件",
  "license": "MIT",
  "homepage": "https://stephenchenorg-astro.netlify.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stephenchenorg/package.astro.git",
    "directory": "packages/graphql"
  },
  "keywords": [
    "astro",
    "graphql",
    "api"
  ],
  "exports": {
    "./api": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.js"
    },
    "./company-setting": {
      "types": "./dist/company-setting/index.d.ts",
      "import": "./dist/company-setting/index.js"
    },
    "./image": {
      "types": "./dist/image/index.d.ts",
      "import": "./dist/image/index.js"
    },
    "./image/components/*": "./dist/image/components/*",
    "./page": {
      "types": "./dist/page/index.d.ts",
      "import": "./dist/page/index.js"
    },
    "./page/components/*": "./dist/page/components/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "tsdown",
    "code-check": "astro check",
    "prepack": "npm run build"
  },
  "peerDependencies": {
    "@stephenchenorg/astro": "^9.0.0",
    "astro": "^5.0.0"
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "awesome-graphql-client": "^2.1.0",
    "graphql": "^16.12.0",
    "graphql-tag": "^2.12.6"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "astro": "^5.16.4",
    "tsdown": "^0.18.1",
    "typescript": "^5.9.3"
  }
}
