{
  "name": "@stephenchenorg/astro",
  "type": "module",
  "version": "1.2.0",
  "description": "Stephenchenorg Astro 前端通用套件",
  "license": "MIT",
  "homepage": "https://stephenchenorg-astro.netlify.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stephenchenorg/package.astro.git"
  },
  "keywords": [
    "astro"
  ],
  "exports": {
    "./api": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.js"
    },
    "./company-setting": {
      "types": "./dist/company-setting/index.d.ts",
      "import": "./dist/company-setting/index.js"
    },
    "./image": {
      "types": "./dist/image/index.d.ts",
      "import": "./dist/image/index.js"
    },
    "./page": {
      "types": "./dist/page/index.d.ts",
      "import": "./dist/page/index.js"
    },
    "./pagination": {
      "types": "./dist/pagination/index.d.ts",
      "import": "./dist/pagination/index.js"
    },
    "./query-params": {
      "types": "./dist/query-params/index.d.ts",
      "import": "./dist/query-params/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "mkdist --declaration --ext=js",
    "lint": "eslint .",
    "code-check": "astro check && npm run lint",
    "prepack": "npm run build",
    "release": "bumpp --commit \"Release v%s\" && npm publish"
  },
  "peerDependencies": {
    "astro": "^5.0.0"
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "awesome-graphql-client": "^2.1.0",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "nanostores": "^0.11.4",
    "query-string": "^9.1.1"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@ycs77/eslint-config": "^4.1.0",
    "astro": "^5.4.1",
    "bumpp": "^10.0.3",
    "eslint": "^9.21.0",
    "eslint-plugin-astro": "^1.3.1",
    "mkdist": "^2.2.0",
    "typescript": "~5.7.3"
  }
}
